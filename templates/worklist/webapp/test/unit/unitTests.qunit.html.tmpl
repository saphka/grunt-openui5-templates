<!DOCTYPE html>
<html>
	<head>
		<title>Unit tests for {{&worklist.parameters.ApplicationTitle.value}}</title>
		<meta http-equiv='X-UA-Compatible' content='IE=edge'/>
		<meta charset="UTF-8">

		<script id="sap-ui-bootstrap"
			src="{{&worklist.environment.resourcePath}}../../resources/sap-ui-core.js"
			data-sap-ui-resourceroots='{
				"{{&worklist.parameters.ApplicationNamespace.value}}": "../../",
				"test.unit": "./"
			}'>
		</script>

		<script>
			jQuery.sap.require("sap.ui.qunit.qunit-css");
			jQuery.sap.require("sap.ui.thirdparty.qunit");
			jQuery.sap.require("sap.ui.qunit.qunit-junit");
			jQuery.sap.require("sap.ui.qunit.qunit-coverage");

			QUnit.config.autostart = false;

			sap.ui.require(
				["test/unit/allTests"],
					function () {
						QUnit.start();
					}
			);
		</script>

	</head>
	<body>
		<div id="qunit"></div>
		<div id="qunit-fixture"></div>
	</body>
</html>